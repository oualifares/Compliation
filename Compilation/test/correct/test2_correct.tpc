int d;
int s;


int trinome(int a, int b, int c) {

    d=b*b-4*a*c;
    if (d>=0) {
        r1=(-b+s)/(2*a);
        r2=(-b-s)/(2*a);
        return 1;
    }
    return 0;
}


int valeur(int a, int b, int c, int x) {
  return a*x*x+b*x+c;
}


